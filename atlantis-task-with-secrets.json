[
  {
    "cpu": 0,
    "secrets": [
      {
        "name": "ATLANTIS_GH_TOKEN",
        "valueFrom": "${ATLANTIS_GH_TOKEN}"
      },
      {
        "name": "ATLANTIS_GH_WEBHOOK_SECRET",
        "valueFrom": "${ATLANTIS_GH_WEBHOOK_SECRET}"
      },
      {
        "name": "ATLANTIS_GITLAB_TOKEN",
        "valueFrom": "${ATLANTIS_GITLAB_TOKEN}"
      },
      {
        "name": "ATLANTIS_GITLAB_WEBHOOK_SECRET",
        "valueFrom": "${ATLANTIS_GITLAB_WEBHOOK_SECRET}"
      }
    ],
    "environment": [
      {
        "name": "ATLANTIS_ALLOW_REPO_CONFIG",
        "value": "${ATLANTIS_ALLOW_REPO_CONFIG}"
      },
      {
        "name": "ATLANTIS_LOG_LEVEL",
        "value": "${ATLANTIS_LOG_LEVEL}"
      },
      {
        "name": "ATLANTIS_PORT",
        "value": "${ATLANTIS_PORT}"
      },
      {
        "name": "ATLANTIS_ATLANTIS_URL",
        "value": "${ATLANTIS_ATLANTIS_URL}"
      },
      {
        "name": "ATLANTIS_GH_USER",
        "value": "${ATLANTIS_GH_USER}"
      },
      {
        "name": "ATLANTIS_GITLAB_USER",
        "value": "${ATLANTIS_GITLAB_USER}"
      },
      {
        "name": "ATLANTIS_REPO_WHITELIST",
        "value": "${ATLANTIS_REPO_WHITELIST}"
      }
    ],
    "essential": true,
    "image": "${atlantis_image}",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${logs_group}",
        "awslogs-region": "${logs_region}",
        "awslogs-stream-prefix": "${logs_stream_prefix}"
      }
    },
    "mountPoints": [],
    "name": "${name}",
    "portMappings": [
      {
        "containerPort": ${ATLANTIS_PORT},
      "hostPort": ${ATLANTIS_PORT},
      "protocol": "tcp"
      }
    ],
    "volumesFrom": []
  }
]
